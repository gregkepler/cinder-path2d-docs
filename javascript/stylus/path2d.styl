.content

	max-width: 680px

ul#sketchLinks

	list-style: none
	padding: 0

	li
		display: inline-block
		margin-right: 5px

	a
		color: #ffffff
		background: #4f4f4f
		padding: 1em
		font-family: sans-serif
		text-decoration: none

canvas
	
	width: 100%
	height: 340px
	outline: #f5f2f0 solid 1px

	&.active
		display: block

	&.inactive
		display: none



button
	
	padding: 1em